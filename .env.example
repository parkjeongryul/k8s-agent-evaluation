# 사내 LLM 서버 설정 (필수 - 평가용)
OPENAI_API_KEY=your_internal_api_key_here
OPENAI_BASE_URL=http://internal-llm.company.com:8000/v1
OPENAI_MODEL=your-internal-model-name

# K8s Agent API 설정 (필수 - 평가 대상)
K8S_AGENT_API_URL=http://k8s-agent.company.com:8080
K8S_AGENT_API_KEY=your_agent_api_key_here  # 인증이 필요한 경우
K8S_AGENT_TIMEOUT=30  # API 타임아웃 (초)

